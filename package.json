{
  "name": "ducen-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "cross-env NODE_ENV=dev nest start --watch",
    "start:local": "cross-env NODE_ENV=local nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/main-api/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/ducen-backend/test/jest-e2e.json"
  },
  "dependencies": {
    "@casl/ability": "^6.3.1",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^2.0.1",
    "@nestjs/core": "^8.0.0",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@superfaceai/passport-twitter-oauth2": "^1.1.0",
    "bcryptjs": "^2.4.3",
    "cache-manager": "^5.1.1",
    "class-transformer": "^0.5.1",
    "cross-env": "^7.0.3",
    "date-fns": "^2.28.0",
    "date-fns-tz": "^1.3.4",
    "express-session": "^1.17.3",
    "firebase-admin": "^11.2.0",
    "fs-extra": "^10.1.0",
    "handlebars": "^4.7.7",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^4.6.0",
    "nodemailer": "^6.8.0",
    "on-headers": "^1.0.2",
    "passport": "^0.6.0",
    "passport-facebook": "^3.0.0",
    "passport-github": "^1.1.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "passport-linkedin-oauth2": "^2.0.0",
    "passport-local": "^1.0.0",
    "passport-twitter": "^1.0.4",
    "passport-twitter-oauth2": "^2.1.1",
    "realm": "^11.0.0",
    "redis": "^4.3.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.5",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "27.5.0",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^16.0.0",
    "@types/nodemailer": "^6.4.6",
    "@types/on-headers": "^1.0.0",
    "@types/passport-facebook": "^2.1.11",
    "@types/passport-github": "^1.1.7",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/passport-jwt": "^3.0.7",
    "@types/passport-linkedin-oauth2": "^1.5.3",
    "@types/passport-local": "^1.0.34",
    "@types/passport-twitter": "^1.0.37",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.0.3",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.1",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@ducen/core(|/.*)$": "<rootDir>/libs/core/src/$1",
      "^@ducen/shared(|/.*)$": "<rootDir>/libs/shared/src/$1",
      "^@ducen/adaptors(|/.*)$": "<rootDir>/libs/adaptors/src/$1"
    }
  }
}

{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/main-api/src",
  "monorepo": true,
  "root": "apps/main-api",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/main-api/tsconfig.app.json"
  },
  "projects": {
    "main-api": {
      "type": "application",
      "root": "apps/main-api",
      "entryFile": "main",
      "sourceRoot": "apps/main-api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/main-api/tsconfig.app.json"
      }
    },
    "core": {
      "type": "library",
      "root": "libs/core",
      "entryFile": "index",
      "sourceRoot": "libs/core/src",
      "compilerOptions": {
        "tsConfigPath": "libs/core/tsconfig.lib.json"
      }
    },
    "event-handler": {
      "type": "application",
      "root": "apps/event-handler",
      "entryFile": "main",
      "sourceRoot": "apps/event-handler/src",
      "compilerOptions": {
        "tsConfigPath": "apps/event-handler/tsconfig.app.json"
      }
    },
    "shared": {
      "type": "library",
      "root": "libs/shared",
      "entryFile": "index",
      "sourceRoot": "libs/shared/src",
      "compilerOptions": {
        "tsConfigPath": "libs/shared/tsconfig.lib.json"
      }
    },
    "adaptors": {
      "type": "library",
      "root": "libs/adaptors",
      "entryFile": "index",
      "sourceRoot": "libs/adaptors/src",
      "compilerOptions": {
        "tsConfigPath": "libs/adaptors/tsconfig.lib.json"
      }
    },
    "cron-scheduler": {
      "type": "application",
      "root": "apps/cron-scheduler",
      "entryFile": "main",
      "sourceRoot": "apps/cron-scheduler/src",
      "compilerOptions": {
        "tsConfigPath": "apps/cron-scheduler/tsconfig.app.json"
      }
    }
  }
}